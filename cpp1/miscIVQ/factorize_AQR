#include <iostream>
#include <vector>

using namespace std;

vector<int> num;

void fun(int n)
{
        int start,x,y;

        if (num.size() == 0)
                start = 2;
        else
                start = num[num.size()-1];

        for (int i=start; i<n; i++)
        {
                x = n/i;
                y = n%i;

                if (x < i)
                        break;
                if (y == 0)
                {
                        for (int j=0; j<num.size(); j++)
                                cout << num[j] << "x";

                        cout << i << "x" << x << endl;
                        num.push_back(i);
                        fun(x);
                        num.pop_back();
                }
        }
}

int main()
{
        int N;
        cout << "enter N :";
        cin >> N;

        fun(N);
}
/*Requirement: https://bintanvictor.wordpress.com/2017/04/16/factorize-a-natural-number-aqr/
Solution from my friend Shanyou.
